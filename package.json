{
  "name": "guheyo-server",
  "version": "1.0.0",
  "description": "monorepo for guheyo-server",
  "author": "keypport",
  "license": "BUSL-1.1",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "build:api": "nest build api",
    "build:bot": "nest build bot",
    "start": "./node_modules/pm2/bin/pm2-runtime ./dist/apps/api/main.js",
    "poststart": "node ./node_modules/pm2/bin/pm2 logs",
    "start:api": "cross-env NODE_ENV=prod node ./dist/apps/api/main.js",
    "start:bot": "cross-env NODE_ENV=prod node ./dist/apps/bot/main.js",
    "start:dev:api": "cross-env NODE_ENV=dev nest start api --watch",
    "start:dev:bot": "cross-env NODE_ENV=dev nest start bot --watch",
    "start:dev:core": "cross-env NODE_ENV=dev nest start core --watch",
    "start:dev:shared": "cross-env NODE_ENV=dev nest start shared --watch",
    "start:debug:api": "cross-env NODE_ENV=dev nest start api --debug --watch",
    "start:debug:bot": "cross-env NODE_ENV=dev nest start bot --debug --watch",
    "start:debug:core": "cross-env NODE_ENV=dev nest start core --debug --watch",
    "start:debug:shared": "cross-env NODE_ENV=dev nest start shared --debug --watch",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:cov": "jest --coverage",
    "prisma:generate": "prisma generate --schema ./libs/prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.345.0",
    "@aws-sdk/s3-request-presigner": "^3.347.1",
    "@prisma/client": "^4.13.0",
    "@types/bluebird": "^3.5.38",
    "@types/continuation-local-storage": "^3.2.4",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.194",
    "@types/mime-types": "^2.1.1",
    "@types/sequelize": "^4.28.15",
    "@types/uuid": "^9.0.2",
    "@types/validator": "^13.7.15",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "fastify": "^4.12.0",
    "fastify-swagger": "^5.2.0",
    "js-yaml": "^4.1.0",
    "nestjs-typeorm-paginate": "^4.0.3",
    "nodeenv": "^3.0.75",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-check-file": "^2.6.1",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "pm2": "^5.3.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  },
  "packageManager": "yarn@3.6.1",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 3.0.0"
  }
}
