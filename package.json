{
  "name": "wtb-server",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "author": "keypport",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node libs/prisma/seed.ts"
  },
  "scripts": {
    "build": "tsc",
    "start": "./node_modules/pm2/bin/pm2-runtime ./dist/server.js",
    "poststart": "node ./node_modules/pm2/bin/pm2 logs",
    "dev": "cross-env NODE_ENV=development nodemon --watch './src/**/*.ts' --exec 'ts-node' ./src/server.ts",
    "migrate:dev": "dotenv -e .env.development prisma migrate dev",
    "migrate:deploy": "dotenv -e .env.production prisma migrate deploy",
    "seed:production": "dotenv -e .env.production ts-node ./prisma/seed.ts",
    "seed:dev": "dotenv -e .env.development ts-node ./prisma/seed.ts",
    "prebuild": "rimraf dist",
    "build:apis": "nest build apis",
    "build:bot": "nest build bot",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:apis": "cross-env NODE_ENV=dev nest start apis",
    "start:dev:apis": "cross-env NODE_ENV=dev nest start apis --watch",
    "start:debug:apis": "cross-env NODE_ENV=dev nest start apis --debug --watch",
    "start:prod:apis": "cross-env NODE_ENV=prod node dist/apps/apis/main",
    "start:bot": "cross-env NODE_ENV=dev nest start bot",
    "start:dev:bot": "cross-env NODE_ENV=dev nest start bot --watch",
    "start:debug:bot": "cross-env NODE_ENV=dev nest start bot --debug --watch",
    "start:prod:bot": "cross-env NODE_ENV=prod node dist/apps/bot/main",
    "lint": "eslint \"{apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:cov": "jest --coverage",
    "prisma:generate": "prisma generate --schema ./libs/prisma/schema.prisma"
  },
  "dependencies": {
    "@fastify/static": "^6.10.2",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-fastify": "^10.0.0",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/typeorm": "^9.0.1",
    "@prisma/client": "^4.13.0",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "fastify": "^4.12.0",
    "fastify-swagger": "^5.2.0",
    "js-yaml": "^4.1.0",
    "nestjs-typeorm-paginate": "^4.0.3",
    "nodeenv": "^3.0.75",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.0",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.2.4",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-check-file": "^2.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "prisma": "^4.13.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.6.1",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 3.0.0"
  }
}
