{
  "name": "wtb-server",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "build": "tsc",
    "start": "./node_modules/pm2/bin/pm2-runtime ./dist/server.js",
    "poststart": "node ./node_modules/pm2/bin/pm2 logs",
    "dev": "cross-env NODE_ENV=development nodemon --watch './src/**/*.ts' --exec 'ts-node' ./src/server.ts",
    "migrate:dev": "dotenv -e .env.development prisma migrate dev",
    "migrate:deploy": "dotenv -e .env.production prisma migrate deploy",
    "seed:production": "dotenv -e .env.production ts-node ./prisma/seed.ts",
    "seed:dev": "dotenv -e .env.development ts-node ./prisma/seed.ts"
  },
  "author": "keypport",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.345.0",
    "@aws-sdk/s3-request-presigner": "^3.347.1",
    "@prisma/client": "^4.13.0",
    "@types/bluebird": "^3.5.38",
    "@types/continuation-local-storage": "^3.2.4",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.194",
    "@types/mime-types": "^2.1.1",
    "@types/sequelize": "^4.28.15",
    "@types/uuid": "^9.0.2",
    "@types/validator": "^13.7.15",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.3",
    "express": "^5.0.0-beta.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nanoid": "^3.0.0",
    "pm2": "^5.3.0",
    "prisma": "^4.13.0",
    "redis": "^4.6.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": ">=8.0.0",
    "eslint-config-airbnb": "latest",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "latest",
    "eslint-config-xo-typescript": "latest",
    "eslint-plugin-check-file": "^2.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.22",
    "prettier": "3.0.0",
    "ts-node": "^10.9.1",
    "typescript": ">=4.7"
  },
  "engines": {
    "node": "please-use-yarn",
    "yarn": ">=3.0.0"
  },
  "packageManager": "yarn@3.6.1"
}
