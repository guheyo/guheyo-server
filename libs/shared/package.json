{
  "name": "@guheyo/shared",
  "version": "1.0.0",
  "author": "keypport",
  "license": "Guheyo Community License (GCL)",
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "scripts": {
    "migrate:dev": "dotenv -e ./src/prisma/config/.env.dev prisma migrate dev",
    "migrate:test": "dotenv -e ./src/prisma/config/.env.test prisma migrate dev",
    "migrate:production": "dotenv -e ./src/prisma/config/.env.production prisma migrate dev",
    "migrate:deploy": "dotenv -e ./src/prisma/config/.env.production prisma migrate deploy",
    "seed:dev": "dotenv -e ./src/prisma/config/.env.dev ts-node ./src/prisma/seeds/seed.ts",
    "seed:test": "dotenv -e ./src/prisma/config/.env.test ts-node ./src/prisma/seeds/seed.ts",
    "seed:production": "dotenv -e ./src/prisma/config/.env.production ts-node ./src/prisma/seeds/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:generate:v": "prisma generate --schema=./src/prisma/schemas/schema-v${0}.prisma",
    "migrate:v": "dotenv -e ./src/prisma/config/.env.v${0} ts-node --project ./tsconfig.lib.json ./src/prisma/scripts/migrate-to-v${0}.ts"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.592.0",
    "@aws-sdk/client-lambda": "^3.592.0",
    "@aws-sdk/client-s3": "^3.417.0",
    "@aws-sdk/client-sso-oidc": "^3.592.0",
    "@aws-sdk/client-sts": "^3.592.0",
    "@aws-sdk/s3-request-presigner": "^3.417.0",
    "@nestjs/common": "^10.4.16",
    "@nestjs/config": "^3.0.1",
    "@nestjs/core": "^10.2.4",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/graphql": "^12.0.8",
    "@prisma/client": "^5.15.0",
    "@types/bcrypt": "^5.0.2",
    "axios": "^1.12.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.11",
    "discord.js": "^14.15.3",
    "express": "^4.20.0",
    "fs": "^0.0.1-security",
    "graphql": "^16.8.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "nanoid": "^3.3.8",
    "prisma": "5.15.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "packageManager": "yarn@3.6.1",
  "devDependencies": {
    "@nestjs/testing": "^10.2.4",
    "@types/mime-types": "^2.1.1",
    "prisma-class-generator": "^0.2.7"
  }
}
